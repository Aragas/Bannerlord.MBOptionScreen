<Brushes>
  <Brush Name="MCMv3.ResetButton">
    <Layers>
      <BrushLayer Name="Default" Sprite="SPGeneral\Nameplates\hover_ring" />
      <BrushLayer Name="Pre161" Sprite="Nameplates\hover_ring" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" ColorFactor="1.0" />
        <BrushLayer Name="Pre161" ColorFactor="1.0" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.3" />
        <BrushLayer Name="Pre161" ColorFactor="1.3" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.85" />
        <BrushLayer Name="Pre161" ColorFactor="0.85" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" ColorFactor="0.2" />
        <BrushLayer Name="Pre161" ColorFactor="0.2" />
      </Style>
    </Styles>
    <SoundProperties>
      <StateSounds>
      </StateSounds>
      <EventSounds>
        <EventSound EventName="Click" Audio="default" />
      </EventSounds>
    </SoundProperties>
  </Brush>
</Brushes>
